  $('.page-files-upload').each(function () {
    var $this = $(this);
    var windowsHight = $(window).height();

    var $fileInputs = $this.find('.files-card').find('input');
    var $fileInputId = $fileInputs.data('id');
    // var $fileRemoveLink = $(`#${$fileInputId}`).;
    // console.log($fileInputId);
    var filesToUpload = [];
    // console.log(filesToUpload.length);
    if (filesToUpload.length === 0) {
      $(`#${$fileInputId}`).prepend('<ul class="uploded-file-List"></ul>')
    }

    $fileInputs.on('change', function (evt) {

      for (var i = 0; i < evt.target.files.length; i++) {
        filesToUpload.push(evt.target.files[i]);
        // filesToUpload.push(evt.target.files[i]);
      }
      var output = [];

      for (var i = 0, uploadfile;
        (uploadfile = evt.target.files[i]); i++) {
        var removeLink =
          '<a class="removeFile" data-fileid="' + i + '"><i class="fa fa-trash icons remove"></i></a>';

