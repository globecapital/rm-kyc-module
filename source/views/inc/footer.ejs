  $('.btn-input-box').each(function () {
    var $this = $(this);
    var $inputs = $this.find('input');
    var $inputId = $inputs.data('id');
    var $inputShow = $inputs.data('show');
    var $inputHide = $inputs.data('hide');
    var $inputValue = $inputs.val().toLowerCase();


    if ($inputs.prop('checked')) {
      $inputs.parents('.btn-input-box').addClass('active');
    }

    $inputs.on('change', function () {
      $(`#${$inputId} .btn-input-box`).removeClass('active');
      $inputs.parents('.btn-input-box').addClass('active');

      if ($inputShow) $(`#${$inputShow}`).show("100");
      if ($inputHide) $(`#${$inputHide}`).hide("100");
      console.log($inputs.val());
    });

  });

  $('.btn-input-check').each(function () {
    var $this = $(this);
    var $inputs = $this.find('input');

    $inputs.filter(':notmdproc')
      .data('mdproc', true)
      .after("<span class='check'></span>");

    if ($inputs.prop('checked')) {
      $inputs.parents(".btn-input-check").addClass('active');
    }
    if ($inputs.prop('disabled')) {
      $inputs.parents(".btn-input-check").addClass('disabled');
    }
    $inputs.on('change click', function () {
      // $inputs.parents(".btn-input-check").addClass('active');
      $inputs.parents(".btn-input-check").addClass('active');

      if (!$inputs.prop('checked')) {
        $inputs.parents(".btn-input-check").removeClass('active');
      }
    });

  });

  function getCodeBoxElement(index) {
    return document.getElementById("codeBox" + index);
  }

  function onKeyUpEvent(index, event) {
    const eventCode = event.which || event.keyCode;
    if (getCodeBoxElement(index).value.length === 1) {
      if (index !== 6) {
        getCodeBoxElement(index + 1).focus();
      } else {
        getCodeBoxElement(index).blur();
        // Submit code
        console.log('submit code ');
      }
    }
    if (eventCode === 8 && index !== 1) {
      getCodeBoxElement(index - 1).focus();
    }
  }

  function onFocusEvent(index) {
    for (item = 1; item < index; item++) {
      const currentElement = getCodeBoxElement(item);
      if (!currentElement.value) {
        currentElement.focus();
        break;
      }
    }
  }
  // timer   

  $('.otp-resend-btn').each(function () {
    let timerOn = true;

    function timer(remaining) {
      var m = Math.floor(remaining / 60);
      var s = remaining % 60;

      m = m < 10 ? '0' + m : m;
      s = s < 10 ? '0' + s : s;
      document.getElementById('timer').innerHTML = m + ':' + s;
      remaining -= 1;

      if (remaining >= 0 && timerOn) {
        document.getElementById('timer').style.display = 'block';
        setTimeout(function () {
          timer(remaining);
        }, 1000);
        return;
      }

      if (!timerOn) {
        // Do validate stuff here
        return;
      }

      // Do timeout stuff here
      document.getElementById('timer').style.display = 'none';
      // $('.resend_otp_dis').removeClass('disable');
      $('.otp-resend-btn').removeClass('disable');
    }

    let otpResendBtn = $(this);
    otpResendBtn.on('click', function (e) {
      e.preventDefault();
      $(this).addClass('disable');
      $('.gc_resend_text').text('resent');
      // $('.resend_otp_dis').addClass('disable');
      timer($(this).data('time'));
    });
  });

