          //console.log("match at: " + i);
          // remove the one element at the index where we get a match
          filesToUpload.splice(i, 1);
        }
      }

      // console.log(fileName);
      // remove the <li> element of the removed file from the page DOM
      $(this).parent().remove();
    });

